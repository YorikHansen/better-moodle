{
    "name": "uzl-better-moodle",
    "description": "Improves the new weird Moodle 4 UI on UzL-Moodle",
    "version": "2.0.0",
    "packageManager": "yarn@4.2.2",
    "type": "module",
    "private": true,
    "scripts": {
        "lint:fix": "yarn eslint . --exit-on-fatal-error --report-unused-disable-directives --cache --cache-strategy content --fix",
        "prettier:write": "prettier --write --cache --cache-strategy content .",
        "prettier:dist": "prettier --write dist --ignore-path $(mktemp)",
        "prettier:i18n": "prettier --write src/i18n",
        "tsm": "typed-scss-modules 'src/style/**/*.module.scss' --nameFormat camel --exportType named --banner '// I am autogenerated. Do NOT modify me!\n\n'",
        "dev": "vite",
        "build": "yarn tsm && tsc && vite build -- --config=uzl && yarn prettier:dist",
        "preview": "vite preview",
        "typesafe-i18n": "typesafe-i18n"
    },
    "dependencies": {
        "classnames": "2.5.1",
        "darkreader": "4.9.86",
        "jsx-dom": "8.1.4",
        "typesafe-i18n": "5.26.2"
    },
    "devDependencies": {
        "@eslint/js": "9.4.0",
        "@types/eslint": "8.56.10",
        "@types/eslint__js": "8.42.3",
        "@types/jquery": "3.5.30",
        "@types/node": "20.14.2",
        "@types/tampermonkey": "5.0.3",
        "eslint": "9.4.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-userscripts": "0.5.3",
        "globals": "15.4.0",
        "prettier": "3.3.1",
        "sass": "1.77.4",
        "typed-scss-modules": "8.0.1",
        "typescript": "5.4.5",
        "typescript-eslint": "7.12.0",
        "typescript-plugin-css-modules": "5.1.0",
        "userscript-meta": "1.0.1",
        "vite": "5.2.13",
        "vite-plugin-monkey": "4.0.3"
    }
}
